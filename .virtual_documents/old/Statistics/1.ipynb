def median(x):
    x = sorted(x)
    
    if len(x)%2 == 1:
        return x[int((len(x)+1)/2) - 1]

    else:    
        index_1 = int(len(x)/2) -1
        index_2 = int((len(x)/2) + 1) -1
        return (x[index_1] + x[index_2]) / 2


