def arrange(df,col):
    points = []
    for i in df[col]:
        points.extend(i)
    points.sort()

    new = [points[0]]

    for i in range(1,round(len(points)/2)+1):
        p1 = points[i]
        p2 = points[i+1]

        p = (max(p1,p2)-min(p1,p2))/2

        new.append(min(p1,p2)+p)
        new.append(max(p1,p2)-p)

    new.append(points[-1])

    final = []
    for i in range(len(new)):
        if i%2 == 0:
            final.append((new[i],new[i+1]))

    return pd.DataFrame({col:final})


